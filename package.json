{
  "name": "vue-stitches",
  "version": "0.0.1-alpha.3",
  "main": "./lib/vue-stitches.common.js",
  "description": "Use stitches in your Vue projects",
  "keywords": ["css-in-js", "css", "stitches", "vue", "nuxt", "stitches/core", "stiches in vue", "vue-stitches"],
  "files": [
    "lib/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "build": "vue-cli-service build --target lib --name vue-stitches ./src/main.js --dest lib"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": ">= 2.5.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dartilesm/vue-stitches"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
